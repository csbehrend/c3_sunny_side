theme_override_font_sizes/normal_font_size = 30
text = "“Just like here, you can be a positive force in your own area! While your community may not have the same problems as mine, chances are there’s some environmental issue close to home.” "

[node name="Scene1_Text3" type="RichTextLabel" parent="."]
visible = false
